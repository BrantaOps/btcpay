    (function() {
        console.log('this so ran');
        // Wait for the payment data to be available
        document.addEventListener('DOMContentLoaded', function() {
            // Modify QR code data
            const qrElements = document.querySelectorAll('[data-qr-value]');
            qrElements.forEach(function(el) {
                const originalValue = el.getAttribute('data-qr-value');
                if (originalValue) {
                    el.setAttribute('data-qr-value', originalValue + '&yourparam=value');
                }
            });
        });
    })();
</script>